{
  "buildCommand": "npx ng build --configuration production",
  "outputDirectory": "dist/cafe-booking-frontend/browser",
  "framework": "angular",
  "routes": [
    {
      "src": "/(.*)\\.(js|css|ico|png|jpg|jpeg|svg|webp|txt|json|wasm|woff2?)",
      "dest": "/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
